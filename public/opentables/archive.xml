<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Capt. Proton</author>
    <documentationURL>http://www.archive.org/advancedsearch.php</documentationURL>
    <description>Searches archive.org for audio by dice media to generate iTunes podcast.</description>
    <sampleQuery>SELECT * FROM {table} WHERE archivequery="dice"</sampleQuery>
  </meta>

  <bindings>
    <select itemPath="" produces="XML">
      <urls>
		<url>http://www.archive.org/advancedsearch.php?q={archivequery}</url>
      </urls>
      <inputs>
        <key id="archivequery" type="xs:string" paramType="path" required="true" />
      </inputs>
    </select>
  </bindings>
</table>
